{
  "name": "react-ui-kanban",
  "version": "1.0.10",
  "description": "React UI Kanban Board",
  "main": "dist/index.js",
  "files": [
    "dist",
    "README"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "storybook": "start-storybook -p 9002",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cover": "istanbul cover -x *.test.js _mocha -- -R spec --require tests/config/setup 'tests/**/*.test.js'",
    "test:report": "cat ./coverage/lcov.info | codecov && rm -rf ./coverage",
    "build": "babel src --out-dir dist --copy-files",
    "docs": "build-storybook -o docs",
    "commit": "git cz",
    "deploy-storybook": "storybook-to-ghpages",
    "format": "pretty-quick \"src/**/*.js\"",
    "size": "size-limit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FelipeBattistotti/react-ui-kanban"
  },
  "keywords": [
    "react",
    "kanban",
    "board",
    "trello"
  ],
  "author": "FelipeBattistotti",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FelipeBattistotti/react-ui-kanban/issues"
  },
  "homepage": "https://github.com/FelipeBattistotti/react-ui-kanban",
  "dependencies": {
    "autosize": "^6.0.1",
    "classnames": "^2.3.2",
    "immutability-helper": "^3.1.1",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "react-popopo": "^2.1.9",
    "react-redux": "^8.1.2",
    "redux": "^4.2.1",
    "redux-actions": "2.6.5",
    "redux-logger": "^3.0.6",
    "trello-smooth-dnd": "1.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/cli": "7.23.0",
    "@babel/core": "^7.23.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-async-to-generator": "7.22.5",
    "@babel/plugin-transform-runtime": "7.22.15",
    "@babel/plugin-transform-template-literals": "^7.22.5",
    "@babel/preset-env": "7.22.20",
    "@babel/preset-react": "7.22.15",
    "@storybook/addon-info": "5.3.21",
    "@storybook/addon-options": "5.3.21",
    "@storybook/addon-storyshots": "7.4.5",
    "@storybook/cli": "7.4.5",
    "@storybook/react": "7.4.5",
    "@storybook/storybook-deployer": "^2.8.16",
    "autoprefixer": "^10.4.16",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-preset-react": "^6.24.1",
    "codecov.io": "^0.1.6",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-standard-react": "^13.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-standard": "^4.1.0",
    "eventsource-polyfill": "^0.9.6",
    "husky": "^8.0.3",
    "i18next": "^23.5.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "jest-styled-components": "^7.1.1",
    "prettier": "3.0.3",
    "pretty-quick": "^3.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.2.2",
    "react-test-renderer": "^18.2.0",
    "semantic-release": "^22.0.5",
    "size-limit": "^9.0.0",
    "styled-components": "6.0.8"
  },
  "peerDependencies": {
    "lodash": ">= 4.17.11",
    "react": "*",
    "react-dom": "*",
    "react-redux": ">= 5.0.7",
    "redux": ">= 4.0.0",
    "redux-logger": ">= 3.0.6",
    "styled-components": ">= 4.0.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "size-limit": [
    {
      "path": "dist/**/*.js",
      "limit": "800 ms"
    }
  ]
}
